    <% if (capsules.length > 0) { %>

        <% for (let capsule of capsules) { %>
            <div class="wrapper">
                <h1><%= capsule.type %></h1>

                <p><%= capsule.serial %></p>
                <p><%= capsule.id %></p>
                <p><%= capsule.last_update %></p>

            </div>
        <% } %>
 

    <% } else { %>
        <h1>No Products Found!</h1>
    <% } %>
    
</body>
